{
  "name": "reactotron-redux-saga",
  "version": "2.1.4",
  "description": "A Reactotron plugin for Redux Saga.",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "reactotron-redux-saga.d.ts"
  ],
  "main": "dist/index.js",
  "types": "./reactotron-redux-saga.d.ts",
  "scripts": {
    "test": "ava",
    "watch": "ava --watch",
    "format": "prettier --write {**,.}/*.js && standard --fix",
    "coverage": "nyc ava",
    "build": "rollup -c ",
    "lint": "standard",
    "ci:test": "yarn test",
    "ci:publish": "yarn semantic-release",
    "semantic-release": "semantic-release"
  },
  "author": "Infinite Red",
  "bugs": {
    "url": "https://github.com/infinitered/reactotron/issues"
  },
  "devDependencies": {
    "@redux-saga/testing-utils": "^1.0.1",
    "@semantic-release/git": "^7.0.5",
    "ava": "^0.20.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "mockery": "^2.1.0",
    "nyc": "^11.0.3",
    "prettier": "^1.10.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.6.1",
    "semantic-release": "^15.12.2",
    "standard": "^10.0.2"
  },
  "homepage": "https://github.com/infinitered/reactotron-redux-saga",
  "license": "MIT",
  "ava": {
    "require": [
      "babel-core/register"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": "https://github.com/infinitered/reactotron-redux-saga",
  "dependencies": {
    "ramda": "^0.24.1",
    "ramdasauce": "^2.0.0",
    "reactotron-core-client": "^2.2.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.0"
  }
}
